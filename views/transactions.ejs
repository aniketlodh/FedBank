<%- include("./partials/header"); %>
<div class="container">
    <table>
        <tr>
            <th>Sl. No</th>
            <th>Sender</th>
            <th>Receiver</th>
            <th>Amount</th>    
        </tr>
        <% transactions.forEach(function(transact,i){ %>
            <tr>
                <td><%= i+1 %></td>
                <td><%= transact.sender %></td>
                <td><%= transact.receiver %></td>
                <td><%= transact.amount %></td>
            </tr>
            <% }) %>
    </table>
</div>
<%- include("./partials/footer"); %>